    "@tiptap/starter-kit": "^3.14.0",
    "bcryptjs": "^3.0.3",
    "firebase-admin": "^13.6.0",
    "jsonwebtoken": "^9.0.3",
    "next": "16.0.10",
    "next-auth": "^4.24.13",
    "pg": "^8.16.3",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "recharts": "^3.6.0"
  },
  "devDependencies": {
    "@prisma/client": "^5.10.2",
